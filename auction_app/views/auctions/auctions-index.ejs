<% include ../partials/boilerplate %>

<a name='top'></a>

<a href='/auctions/add' class='sell'>Sell something!</a>

<div class='auctions'>

  <% auctionData.forEach(function(auction) { %>
    <div class='listing'>
      <h3><%= auction.item %></h3>
      <div class='meta'>
        <span class='status'><%= auction.status %></span>
      </div>
      <a href='/auctions/<%= auction.id %>'>Go to auction</a>
      <a href='/auctions/edit/<%= auction.id %>'>Edit auction</a>
      <form method='POST' action='/auctions/<%= auction.id %>?_method=DELETE'>
        <input type='submit' value='Delete it' />
      </form>
    </div>
  <% }) %>

<a href='#top' class='top'>Back to top</a>

</div>


<% include ../partials/end %>